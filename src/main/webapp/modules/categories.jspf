<header>{{ node.title }}<span> - {{node.description}}</span></header>
<section data-ng-if="node.memories">
	<article class="thumbnail" data-ng-repeat="memory in node.memories" data-ng-click="view(memory.id, memory.title)">
		<img src="/img/design/memo_no_image.png" alt="image"/>
		<footer>{{memory.title | truncate:20}}</footer>
	</article>
</section>
<div data-ng-repeat="node in node.children" data-ng-include="'/modules/categories.jspf'"></div>